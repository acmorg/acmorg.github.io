---
sitemap: false
---
[
	{% for organizer in site.data.organizers %}
		{
		  "name"       : "{{ organizer.name }}",
		  "city"  		 : "{{ organizer.city }}",
			"city_long"  : "{{ organizer.city_long }}",
      "specialty"  : "{{ organizer.specialty }}",
		  "url"        : "/cities/{{ organizer.city_slug }}"
		} {% unless forloop.last %},{% endunless %}
	{% endfor %}
]
